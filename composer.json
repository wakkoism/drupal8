                "installer-name": "google_charts"
              },
              "dist": {
                "url": "https://www.gstatic.com/charts/loader.js",
                "type": "file"
              }
            }
       },
       {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.5.0",
                "type": "drupal-library",
