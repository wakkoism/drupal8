    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "desandro/masonry": "^4.2",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/charts": "3.x-dev",
        "drupal/colorbox": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.1",
        "drupal/devel": "~1.0",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^1.5",
        "drupal/entity_embed": "1.x-dev",
        "drupal/field_group": "3.x-dev",
        "drupal/file_browser": "^1.1",
        "drupal/file_entity": "2.x-dev",
        "drupal/juicerio": "1.x-dev",
        "drupal/libraries": "3.x-dev",
        "drupal/migrate_manifest": "^1.7",
        "drupal/migrate_run": "1.x-dev",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.2",
        "drupal/workbench": "^1.0",
        "drupal/workbench_access": "1.x-dev",
        "drupal/wp_migrate": "^1.1",
        "drush/drush": "^9.0.0",
        "google/charts": "^45.0",
        "highcharts/3d": "6.1.0",
        "highcharts/accessibility": "6.1.0",
        "highcharts/export-data": "6.1.0",
        "highcharts/exporting": "6.1.0",
        "highcharts/highcharts": "6.1.0",
        "highcharts/more": "6.1.0",
        "jackmoore/colorbox": "^1.6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
